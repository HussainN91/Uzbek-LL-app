  "strings": [
    {
      "id": "dialogues.U10_L01_D01.line_118",
      "field": "line_uz",
      "value": "Salom Bekzod! Sen nima sotib olayapsan?",
      "context": {
        "line_number": 118,
        "field": "line_uz",
        "speaker": "Nilufar",
        "dialogue_id": "U10_L01_D01",
        "en_line": "Hi Bekzod! What are you buying?"
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_119",
      "field": "line_uz",
      "value": "Men pechenye sotib olayapman. Nonushta uchun.",
      "context": {
        "line_number": 119,
        "field": "line_uz",
        "speaker": "Bekzod",
        "dialogue_id": "U10_L01_D01",
        "en_line": "I'm buying biscuits. For breakfast."
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_120",
      "field": "line_uz",
      "value": "Yaxshi! Men pishloq sotib olayapman. Tushlik uchun.",
      "context": {
        "line_number": 120,
        "field": "line_uz",
        "speaker": "Nilufar",
        "dialogue_id": "U10_L01_D01",
        "en_line": "Good! I'm buying cheese. For lunch."
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_121",
      "field": "line_uz",
      "value": "Zo'r! Bu sirg'alar-chi? Kimga?",
      "context": {
        "line_number": 121,
        "field": "line_uz",
        "speaker": "Bekzod",
        "dialogue_id": "U10_L01_D01",
        "en_line": "Great! What about these earrings? For whom?"
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_122",
      "field": "line_uz",
      "value": "Bu sirg'alar onam uchun. Uning tug'ilgan kuni uchun.",
      "context": {
        "line_number": 122,
        "field": "line_uz",
        "speaker": "Nilufar",
        "dialogue_id": "U10_L01_D01",
        "en_line": "These earrings are for my mother. For her birthday."
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_123",
      "field": "line_uz",
      "value": "Chiroyli! Bu hamyon seniki-mi?",
      "context": {
        "line_number": 123,
        "field": "line_uz",
        "speaker": "Bekzod",
        "dialogue_id": "U10_L01_D01",
        "en_line": "Beautiful! Is this wallet yours?"
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_124",
      "field": "line_uz",
      "value": "Ha, bu hamyon meniki. Men uni kecha sotib oldim.",
      "context": {
        "line_number": 124,
        "field": "line_uz",
        "speaker": "Nilufar",
        "dialogue_id": "U10_L01_D01",
        "en_line": "Yes, this wallet is mine. I bought it yesterday."
      }
    },
    {
      "id": "dialogues.U10_L01_D01.line_125",
      "field": "line_uz",
      "value": "Yaxshi tanlov! Endi to'lash vaqti!",
      "context": {
        "line_number": 125,
        "field": "line_uz",
        "speaker": "Bekzod",
        "dialogue_id": "U10_L01_D01",
        "en_line": "Good choice! Now it's time to pay!"
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_145",
      "field": "line_uz",
      "value": "Salom Otabek! Sen nima sotib oldingiz?",
      "context": {
        "line_number": 145,
        "field": "line_uz",
        "speaker": "Madina",
        "dialogue_id": "U10_L02_D01",
        "en_line": "Hi Otabek! What did you buy?"
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_146",
      "field": "line_uz",
      "value": "Men biroz pechenye sotib oldim. Bir nechta.",
      "context": {
        "line_number": 146,
        "field": "line_uz",
        "speaker": "Otabek",
        "dialogue_id": "U10_L02_D01",
        "en_line": "I bought some biscuits. A few."
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_147",
      "field": "line_uz",
      "value": "Yaxshi! Men biroz pishloq sotib oldim. Ozgina.",
      "context": {
        "line_number": 147,
        "field": "line_uz",
        "speaker": "Madina",
        "dialogue_id": "U10_L02_D01",
        "en_line": "Good! I bought some cheese. A bit."
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_148",
      "field": "line_uz",
      "value": "Sen biroz pul sarfladingizmi? Ko'pmi?",
      "context": {
        "line_number": 148,
        "field": "line_uz",
        "speaker": "Otabek",
        "dialogue_id": "U10_L02_D01",
        "en_line": "Did you spend some money? A lot?"
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_149",
      "field": "line_uz",
      "value": "Yo'q, ko'p emas. Men ozgina pul sarfladim.",
      "context": {
        "line_number": 149,
        "field": "line_uz",
        "speaker": "Madina",
        "dialogue_id": "U10_L02_D01",
        "en_line": "No, not a lot. I spent a bit of money."
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_150",
      "field": "line_uz",
      "value": "Men ham. Men bir nechta tanga oldim.",
      "context": {
        "line_number": 150,
        "field": "line_uz",
        "speaker": "Otabek",
        "dialogue_id": "U10_L02_D01",
        "en_line": "Me too. I received a few coins."
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_151",
      "field": "line_uz",
      "value": "Chek oldingizmi?",
      "context": {
        "line_number": 151,
        "field": "line_uz",
        "speaker": "Madina",
        "dialogue_id": "U10_L02_D01",
        "en_line": "Did you get the receipt?"
      }
    },
    {
      "id": "dialogues.U10_L02_D01.line_152",
      "field": "line_uz",
      "value": "Ha, men chekni oldim. U meniki.",
      "context": {
        "line_number": 152,
        "field": "line_uz",
        "speaker": "Otabek",
        "dialogue_id": "U10_L02_D01",
        "en_line": "Yes, I got the receipt. It's mine."
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_172",
      "field": "line_uz",
      "value": "Salom Sardor! Bu sumka qancha?",
      "context": {
        "line_number": 172,
        "field": "line_uz",
        "speaker": "Gulnora",
        "dialogue_id": "U10_L03_D01",
        "en_line": "Hi Sardor! How much is this bag?"
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_173",
      "field": "line_uz",
      "value": "Bu sumka 50 dollarga. Juda qimmat!",
      "context": {
        "line_number": 173,
        "field": "line_uz",
        "speaker": "Sardor",
        "dialogue_id": "U10_L03_D01",
        "en_line": "This bag is for 50 dollars. Very expensive!"
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_174",
      "field": "line_uz",
      "value": "Juda qimmat! Men eski sumkamni 20 dollarga sotdim.",
      "context": {
        "line_number": 174,
        "field": "line_uz",
        "speaker": "Gulnora",
        "dialogue_id": "U10_L03_D01",
        "en_line": "Very expensive! I sold my old bag for 20 dollars."
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_175",
      "field": "line_uz",
      "value": "Yaxshi! Bu ish dushanba kuni uchun tugashi kerak.",
      "context": {
        "line_number": 175,
        "field": "line_uz",
        "speaker": "Sardor",
        "dialogue_id": "U10_L03_D01",
        "en_line": "Good! This work must be finished for Monday."
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_176",
      "field": "line_uz",
      "value": "Aytgancha, Ispaniya ovqati bilan mashhur.",
      "context": {
        "line_number": 176,
        "field": "line_uz",
        "speaker": "Gulnora",
        "dialogue_id": "U10_L03_D01",
        "en_line": "By the way, Spain is famous for food."
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_177",
      "field": "line_uz",
      "value": "Ha! Va 'Sneakers' - bu 'trainers' uchun so'z.",
      "context": {
        "line_number": 177,
        "field": "line_uz",
        "speaker": "Sardor",
        "dialogue_id": "U10_L03_D01",
        "en_line": "Yes! And 'Sneakers' - this is the word for 'trainers'."
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_178",
      "field": "line_uz",
      "value": "To'g'ri! Men chegirma uchun so'rayman.",
      "context": {
        "line_number": 178,
        "field": "line_uz",
        "speaker": "Gulnora",
        "dialogue_id": "U10_L03_D01",
        "en_line": "Right! I'll ask for a discount."
      }
    },
    {
      "id": "dialogues.U10_L03_D01.line_179",
      "field": "line_uz",
      "value": "Yaxshi fikr! Chekni oling!",
      "context": {
        "line_number": 179,
        "field": "line_uz",
        "speaker": "Sardor",
        "dialogue_id": "U10_L03_D01",
        "en_line": "Good idea! Get the receipt!"
      }
    },
